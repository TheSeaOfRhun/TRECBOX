import sys, random

def knuth_shuffle(a):
    a_ = list(a)
    n = len(a_)
    i = n - 1
    while (i >= 1):
        j = randint(0, i)
        t = a_[j]
        a_[j] = a_[i]
        a_[i] = t
        i -= 1
    return set(a_)

def main(argv):
    a = [1,2,3,4,5,6]
    print knuth_shuffle(set(a))
    

if __name__ == "__main__":
    main(sys.argv)


